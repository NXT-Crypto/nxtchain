start := time.Now()

lblock, err := nxtblock.GetLatestBlock(blockDir, false)
if err != nil {
	fmt.Printf("Error getting latest block: %v\n", err)
	return
}

block, err := nxtblock.NewBlock(transactions, ruleset, "minerAddr", "NXT", "DATA", lblock)

if err != nil {
	fmt.Printf("Error creating block: %v\n", err)
	return
}
elapsed := time.Since(start)
fmt.Printf("\n-- Done! (%s) - %s\n", elapsed, block.Hash)

// * 6. Block validieren
// ? Block ID und Hash korrekt? (Nachbilden und vergleichen)
// ? Previous Hash korrekt? (Vorheriger Block)
// ? Timestamp korrekt? (Nicht zukunft)
// ? Block Height korrekt? (Vorheriger Block + 1)
// ? Transaktionhash korrekt? (Alle Transaktionen)
// ? Anzahl (Nicht mehr als MaxTransactions)
// ? Jede Transaktion einmalig? (Double Spending)
// ? Jede Transaktion gltig? (Transaktionen validieren)
// ? Blockhash korrekt? (Proof of Work)
// ? Blockgebhr und Belohnung korrekt? (Block Reward)
// ? Sind alle UTXO Inputs gelscht? (Lokale UTXO Datenbank checken)

fmt.Printf("\rVerifying block...")
_, err = nxtblock.ValidatorValidateBlock(*block, blockDir, ruleset)
if err != nil {
	fmt.Printf("\nError validating block: %v\n", err)
	return
}
fmt.Printf("\n[+] BLOCK IS VALID | YOU'VE EARNED %f NXT (%f)\n", nxtblock.ConvertAmount(block.HeadTransactions[0].Outputs[0].Amount), nxtblock.ConvertAmount(nxtblock.CalculateBlockFee(block.Transactions)))

// * 7.  UTXO Inputs lschen & Outputs hinzufgen in die UTXO Datenbank
nxtblock.DeleteBlockUTXOs(block.Transactions)
nxtblock.ConvertBlockToUTXO(*block)

fmt.Print(nxtutxodb.UTXODatabase)

// * 8. Block in die Blockchain einfgen & Block an alle Peers senden

path := nxtblock.SaveBlock(*block, blockDir)
if err != nil {
	fmt.Printf("Error saving block: %v\n", err)
	return
}

fmt.Printf("\nBlock saved to %s\n", path)
